[
    {
      "name": "rule-rewrite-bizay-access-token",
      "rewrite_rule": [
        {
          "name": "NewRewrite",
          "rule_sequence": 100,
          "condition": {
            "variable": "http_req_bizay-access-token",
            "pattern": "PBJHf4FhpJgaEAm8",
            "ignore_case": true,
            "negate": true
          },
          "url": {
            "path": "/default",
            "components": "path_only",
            "reroute": true
          }
        }
      ]
    }
]